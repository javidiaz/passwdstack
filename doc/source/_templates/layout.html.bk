{% extends "basic/layout.html" %}

{% block rootrellink %}
        <li><a href="{{ pathto('index') }}">Projects</a>|&nbsp;</li>
        <li><a href="https://portal.futuregrid.org/">Portal</a>|&nbsp;</li>
{% endblock %}


{% block relbar1 %}

<div style="background-image:url('_static/header_bg.png');background-color: black; text-align: left; padding: 10px 10px 15px 35px">
<table>
  <tr>
    <td>
      <a href="{{ pathto('index') }}"><img src="{{pathto("_static/fg-logo-grid.png", 1) }}" border="0" alt="FutureGrid Move"/></a>
    </td>
    <td>
      <font size="5" color="white"><b><i>FutureGrid</i></b></font><br><br>
      <font size="10" color="white" face="Comic Sans MS"><b><i>Move: Cloud Shifting</i></b></font>
    </td>
  </tr>
</table>

</div>
{{ super() }}
{% endblock %}


{# put the sidebar before the body #}
{% block sidebar1 %}{{ sidebar() }}{% endblock %}
{% block sidebar2 %}{% endblock %}

{%- block document%}
<body style="background-image:url('_static/header_bg.png');background-color: grey;">
</body>
{{ super() }}
{% endblock %}

